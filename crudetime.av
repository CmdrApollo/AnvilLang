var times: float = 0.0;
var n: int = 100; # number of trials
var valueCount: int = 1000;

loop (unpack(n)) -> (_: int) {
    var startTime: float = getTime();

    loop (unpack(valueCount)) -> (i: int) {
        println(mathSin(i + 0.0));
    }

    times = times + getTime() - startTime;
}

println("Average time to print " + toString(valueCount) + " values over " + toString(n) + " trials:");
println(toString(times / n) + " seconds.");